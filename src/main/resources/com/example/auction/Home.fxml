<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.auction.HelloController">
   <children>
      <Button fx:id="A" layoutX="253.0" layoutY="52.0" mnemonicParsing="false" onAction="#goToAuction" prefHeight="25.0" prefWidth="108.0" text="Add Auction Lot" />
      <Button fx:id="bid" layoutX="253.0" layoutY="94.0" mnemonicParsing="false" onAction="#goToBidder" prefHeight="25.0" prefWidth="108.0" text="Add bidder" />
      <Button fx:id="addbid" layoutX="252.0" layoutY="136.0" mnemonicParsing="false" onAction="#goToAddBid" prefHeight="25.0" prefWidth="108.0" text="Add Bid" />
      <Button fx:id="Sell" layoutX="252.0" layoutY="181.0" mnemonicParsing="false" onAction="#goToSell" prefHeight="25.0" prefWidth="108.0" text="Sell Lot" />
      <Button fx:id="Edit"  layoutX="252.0" layoutY="225.0" mnemonicParsing="false" onAction="#goToEdit" prefHeight="25.0" prefWidth="108.0" text="Search Lot/Bidder" />
   </children>
</AnchorPane>
