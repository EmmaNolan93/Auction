<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.auction.AddAuctionLotController">
   <children>
      <TextField fx:id="title" onKeyPressed="#name" layoutX="88.0" layoutY="28.0" />
