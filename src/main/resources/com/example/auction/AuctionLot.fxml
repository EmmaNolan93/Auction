      <Label layoutX="7.0" layoutY="215.0" prefHeight="17.0" prefWidth="77.0" text="Estimate year:" />
      <Label layoutX="7.0" layoutY="256.0" prefHeight="17.0" prefWidth="77.0" text="Asking Price:" />
