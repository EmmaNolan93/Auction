      <CheckBox fx:id="box3" layoutX="247.0" layoutY="256.0" mnemonicParsing="false" />
      <ListView fx:id="view" layoutX="337.0" onMousePressed="#selectLot" layoutY="215.0" prefHeight="179.0" prefWidth="221.0" />
      <Button fx:id="Update" onAction="#update" layoutX="154.0" layoutY="320.0" mnemonicParsing="false" text="Update" />
